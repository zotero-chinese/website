  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "@highcharts/dashboards": "1.3.1",
    "@mdit/plugin-footnote": "0.9.0",
    "element-plus": "2.7.1",
    "feed": "4.2.2",
    "highcharts": "11.4.1",
    "highcharts-vue": "2.0.1",
    "vitepress": "1.1.3",
    "vue": "3.4.23"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.12.7",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.23.0",
    "fs-extra": "11.2.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "mermaid": "10.9.0",
    "prettier": "3.2.5",
    "radash": "12.1.0",
    "typescript": "5.4.5",
    "unplugin-auto-import": "0.17.5",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.1.6",
    "vue-tsc": "2.0.13"
  },
  "lint-staged": {
    "*.md": "prettier --ignore-unknown --write"
  }
